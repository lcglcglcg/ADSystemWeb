.set_view_create_cont .scommon_title a{ display:block; width:13px; height:13px;float:right; background:url(/static/ads_c_2.0/web/images/step_close.gif) no-repeat; margin-top:10px;}
/* 第一步头部样式 */
.scommon_main .all_title span{ display:block; float:left; width:100px; padding-left:10px; height:28px; background:url(/static/ads_c_2.0/web/images/setp_bg1.gif) no-repeat right center;}
.scommon_main .all_title span.step_curr{background: #c9e2ff url(/static/ads_c_2.0/web/images/setp_bg2.gif) no-repeat right center; color:#0066cc;}

/* 第一步内容样式 */
.step1_cont{ overflow:hidden; clear:both; margin-top:10px;}
.step1_cont ul li { padding-top:10px; float:left; width:100%;}
.step1_cont ul li.nopad { padding-top:5px;float:left; width:100%;}
.step1_cont ul li span.step1_lf{ display:block; width:100px; text-align:right; float:left; padding-left:30px; position:relative;}
.step1_cont ul li span.step1_lf1{ display:block; width:70px; text-align:right; float:left;position:relative;}
.step1_cont ul li span.step1_lf2{ display:block; width:100px; text-align:right; float:left; padding-left:0px; position:relative;}
.step1_cont ul li span.step1_rg{ display:block; width:700px; float:left;}
.step1_cont ul li span.step1_rg1{ display:block;float:left;}
.step1_cont ul li span.step1_rg input{ margin-right:5px;}
.step1_cont ul li span.step1_rg label{ margin-right:10px;}
.step1_cont ul li span.step1_rg label input{ vertical-align:middle;}

.step1_cont ul li span.step1_rg2{ height:24px; line-height:24px; max-width:220px; overflow:hidden;}
.step1_cont ul li span.step1_rg2 img.img_false{float:left; margin:5px 5px 0 5px;}

/* 品牌广告-设置创意 */
