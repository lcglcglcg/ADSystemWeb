/* CSS Document */
a img{ text-decoration:none; cursor:pointer;}
.set_view_create_cont .scommon_title{ height:35px; background:#5398f6 url(/static/ads_c_2.0/web/images/step_rg_bg.png) no-repeat right center; padding:0 10px;}
