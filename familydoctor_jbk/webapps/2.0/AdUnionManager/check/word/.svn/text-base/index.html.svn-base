                  <option value="3" selected="selected">修改时间</option>
                  <option value="4">状态</option>
                </select>
            </li>
             <li class="conditions_li padl2">
                 <select name="SORT_TYPE">
                  <option value="1">升序</option>
                  <option value="2" selected="selected">降序</option>
                </select>
            </li>
            <li class="conditions_li padl2"><label><input checked="checked" type="checkbox" id="BALANCE_TYPE" value="2" class="checkbox_01" />隐藏账户无余额记录</li>
